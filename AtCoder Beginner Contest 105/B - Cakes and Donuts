import java.util.Scanner;

public class Main{

static Scanner in=new Scanner(System.in);

public static void main(String[] amit){

int n=in.nextInt();

boolean ok=false;

for (int j = 0; j <= n / 4; j++) {
		for (int k = 0; k <= n / 7; k++) {
			if (4 * j + 7 * k == n){ 
ok = true;

break;

}	

	}
	}

if( !ok ){

System.out.println("No");

}else{

System.out.println("Yes");

}

}

}
