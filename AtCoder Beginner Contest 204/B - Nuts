import java.util.Scanner;

public class Main{

  static Scanner in=new Scanner(System.in);
  
public static void main(String[] amit){

  int n=in.nextInt();

  int a[]=new int[n];
  
  int ans=0;
  
  for(int i=0;i<n;i++){
  
    a[i]=in.nextInt();
    
    if(a[i]>10){
    
      ans+=a[i]-10;
      
    }
    
  }
  
  System.out.println(ans);
  
}
  
}
