import java.util.Scanner;

public class Main{
 
  static Scanner in=new Scanner(System.in);
  
  public static void main(String[] amit){
  
    int x=in.nextInt();
    int y=in.nextInt();
  
    if(x==y){
    
      System.out.print(x);
      
    }else{
    
    for(int i=0;i<3;i++){
    
      if(i!=x && i!=y){
      
        System.out.println(i);
        
        return;
        
      }
      
    }
      
    }
    
  }
  
}
